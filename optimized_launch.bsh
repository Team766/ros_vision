#!/bin/bash

CORES="4,5"
PRIORITY=90

echo "Setting the clocks to max!"
sudo jetson_clocks

echo
echo
echo "Launching vision system on cores ${CORES} @ priority ${PRIORITY}.."

sudo taskset -c ${CORES} chrt -f ${PRIORITY} bash -c "source install/setup.bash && source build_env_vars.sh && ros2 launch ros_vision_launch launch_vision.py"

